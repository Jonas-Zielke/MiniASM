# command_test.masm

# MOV Befehl testen
MOV R0, 5
OUT R0

# ADD Befehl testen
ADD R0, 3
OUT R0

# SUB Befehl testen
SUB R0, 2
OUT R0

# CMP und JEQ Befehl testen
CMP R0, 6
JEQ equal_label
OUT R1  # Diese Zeile sollte nicht ausgeführt werden
JMP end_label

equal_label:
OUT R0  # Diese Zeile sollte ausgeführt werden

end_label:

# SETPIX Befehl testen (Pixel setzen)
MOV R1, 10  # X-Koordinate
MOV R2, 20  # Y-Koordinate
MOV R3, 255  # Farbe (maximal)
SETPIX R1, R2, R3

# Zeitmessen und Ergebnis ausgeben
TIME R4
OUT R4

# Beende das Programm
HLT
