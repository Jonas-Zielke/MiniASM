# calculator.masm

# Lade die erste Zahl in R0
MOV R0, 15  # Beispiel: 15

# Lade die zweite Zahl in R1
MOV R1, 27  # Beispiel: 27

# Addiere R1 zu R0 und speichere das Ergebnis in R2
ADD R0, R1
MOV R2, R0  # Das Ergebnis wird in R2 gespeichert

# Gib das Ergebnis aus
OUT R2

# Beende das Programm
HLT
